  "main": "src/app-lambda.ts",
  "description": "",
  "private": true,
  "scripts": {
    "compile_ts": "rm -rf ./build/* && tsc && cp package.json yarn.lock ./build && cd build && yarn --production --ignore-optional",
    "compile_esbuild": "rm -rf ./build/* && yarn esbuild ./src/app-lambda.ts --platform=node --bundle --outfile=build/src/app-lambda.js",
    "deploy": "yarn compile_webpack && yarn zip && AWS_PROFILE=inves-technology terraform -chdir='./infrastructure' workspace select dev && AWS_PROFILE=inves-technology terraform -chdir='./infrastructure' apply",
    "dev": "nodemon",
    "typecheck": "tsc",
    "generate": "graphql-codegen",
    "lint": "eslint --max-warnings=0 './src/**/*.ts' ",
    "test": "jest",
    "zip": "rm -f ./build/deploy.zip && cd build && zip -r deploy.zip *"
  },
  "dependencies": {
